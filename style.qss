/*FILES ZONE WIDGET ############################################## */

#object_item_label {
    color:#FFF;
}

#explorer_frame {
    background-color: #202020;
    border-radius: 13px;
}

/* ================================== */

#file_frame {
    background-color: #202020;
  
}

#object_item_widget {
    background-color: #3c3c3c;
    border: 1px solid transparent;
    margin: 0;
    padding: 0;
    border-radius: 11px;

    font-family: "Calibri";
    font-size: 4px;
    color:#FFF;
}

#object_item_widget:hover {
    background-color: #656565;
    border: 1px solid transparent;
}

#object_item_widget_selected {
    background-color: #007aa8;
    border: 1px solid #3ec2ff;
    margin: 0;
    padding: 0;
    border-radius: 11px;
    color:#FFF;


    font-family: "Calibri";
    font-size: 4px;
}

#object_item_widget_selected:hover {
    background-color: #0093ca;
    border: 1px solid #3ec2ff;
}

