#window{
 background-color: transparent;

 
}

QScrollArea {
    background-color: transparent;
    border-radius: 13px;
}

/* NAVIGATION WIDGET ################################ */

#top_widget {
    background-color: #202020;
    border-radius: 13px;
}


/* Button ------------------------------------------------ */



#top_widget QPushButton {
    background-color: #3c3c3c;
    border: 1px solid transparent;
    padding: 7px;
    border-radius: 13px;
    padding: 7px 14px;

    font-family: "Calibri";
    font-size: 15px;
    color:#FFF;
}

#top_widget QPushButton:hover {
    background-color: #656565;
    border: 1px solid transparent;
    border-radius: 13px;
}
/* Input ------------------------------------------------ */
#top_widget QLineEdit {
    background-color: #3c3c3c;
    border: 1px solid transparent;
    padding: 7px;
    border-radius: 13px;

    font-family: "Calibri";
    font-size: 15px;
    color:#FFF;
}

#top_widget QLineEdit:hover {
    background-color: #656565;
    border: 1px solid transparent;
    
}

